{
  "include": ["./**/*.ts"],
  "compilerOptions": {
    "types": ["cypress", "node"],
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "target": "ESNext",
    "module": "esnext",
    "declaration": true,
    "strict": true,
    "noImplicitAny": false,
    "outDir": "./dist",
  }
}
