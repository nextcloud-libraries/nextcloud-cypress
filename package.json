{
  "name": "@nextcloud/cypress",
  "version": "1.0.0",
  "description": "Nextcloud cypress commands, utils and selectors library",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "author": "John Molakvo√¶ <skjnldsv@protonmail.com>",
  "contributors": [],
  "license": "AGPL-3.0-or-later",
  "keywords": [
    "nextcloud",
    "cypress",
    "selector",
    "plugins",
    "library"
  ],
  "scripts": {
    "build": "rollup --config rollup.config.js",
    "build:doc": "typedoc --out dist/doc lib && touch dist/doc/.nojekyll",
    "dev": "echo 'No dev build available, production only' && npm run build",
    "watch": "rollup --config rollup.config.js --watch",
    "test": "jest",
    "test:watch": "jest --watchAll --verbose true",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nextcloud/nextcloud-cypress.git"
  },
  "bugs": {
    "url": "https://github.com/nextcloud/nextcloud-cypress/issues"
  },
  "engines": {
    "node": "^14.0.0",
    "npm": "^7.0.0"
  },
  "homepage": "https://github.com/nextcloud/nextcloud-cypress",
  "devDependencies": {
    "@rollup-extras/plugin-clean": "^1.2.3",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/jest": "^28.1.1",
    "cypress": "^10.2.0",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "rollup": "^2.75.6",
    "ts-jest": "^28.0.4",
    "tslib": "^2.4.0",
    "typedoc": "^0.22.17",
    "typescript": "^4.7.3"
  },
  "dependencies": {
  }
